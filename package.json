{
  "name": "simple-auth",
  "version": "1.0.0",
  "description": "Simple auth app using MEVN stack",
  "author": "Petr Chatrn√Ω",
  "scripts": {
    "build": "npm install --prefix ./frontend/ && npm run build --prefix ./frontend/",
    "backend": "nodemon app.js",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "npm run serve --prefix ./frontend"
  },
  "main": "app.js",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.0",
    "morgan": "^1.10.0",
    "path-to-regexp": "^6.2.1"
  },
  "devDependencies": {
    "@vue/cli-service": "^5.0.8",
    "concurrently": "^7.2.2",
    "nodemon": "^2.0.18",
    "vue-cli-plugin-tailwind": "~3.0.0"
  },
  "keywords": [
    "MongoDB",
    "Express.js",
    "Vue.js",
    "Node.js",
    "JWT",
    "Vuex"
  ],
  "license": "ISC"
}
